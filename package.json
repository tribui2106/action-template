{
  "name": "typescript-action",
  "version": "1.0.4",
  "private": true,
  "description": "TypeScript template action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build",
    "test": "jest --coverage",
    "all": "npm run build && npm run format && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mtbui01/action-template.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "1.2.6",
    "@actions/tool-cache": "1.6.1"
  },
  "devDependencies": {
    "@types/jest": "26.0.20",
    "@types/node": "14.14.34",
    "@typescript-eslint/parser": "4.19.0",
    "@vercel/ncc": "0.27.0",
    "eslint": "7.22.0",
    "eslint-plugin-github": "4.1.2",
    "eslint-plugin-jest": "24.3.1",
    "jest": "26.6.3",
    "jest-circus": "26.6.3",
    "jest-sonar-reporter": "2.0.0",
    "sonarqube-scanner": "2.8.0",
    "js-yaml": "4.0.0",
    "prettier": "2.2.1",
    "ts-jest": "26.5.3",
    "typescript": "4.2.3"
  }
}
